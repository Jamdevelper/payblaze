	<script type="text/javascript">

		// Used for Chrome 29.0.1547.76 m  Fix unitl they can properly handle a 304 content_type header on a cached 304 css 
		function reloadStylesheets() {
		    var queryString = '?reload=' + new Date().getTime();
		    $('link[rel="stylesheet"]').each(function () {
		        this.href = this.href.replace(/\?.*|$/, queryString);
		    });
		}
		$.browser.chrome = /chrom(e|ium)/.test(navigator.userAgent.toLowerCase());
    	
    	//Fix call chrome only fix    	
    	if($.browser.chrome){
			reloadStylesheets() ;
		}

	    $(document).ready(function () {
	    
	        //FIX login link;
		    $('a.editor-login').each(function () {
		        if(window.location.protocol!= "https"){
		        	this.href = "https://" + window.location.hostname + '/_layouts/authenticate.aspx?source=' + encodeURIComponent("https:" + window.location.href.substring(window.location.protocol.length));
		        }else{
		        this.href = '/_layouts/authenticate.aspx?source=' + encodeURIComponent(window.location.href);
		        }    
		    });

	    	//LEFT NAV STYLING
	    	// add "selected-parent" class
	    	//   to the parent li and a elements of the selected node 
	    	//   at least one level below the root
	    	$('#left-nav ul > li > ul > li li.selected').closest('ul')
	    		.siblings('a').addClass('selected-parent')
	    		.parent().addClass('selected-parent');
	    	// hide branches
	    	//   that do not contain the selected node
	    	//   except for the immediate child branch of the selected node
	    	$('#left-nav ul:not(:has(li.selected))').css('display','none');
		   	$('#left-nav li.selected > ul').css('display','block');
	    	// show (animate) the menu
	    	//   which is hidden by default
	    	//   if javascript is enabled
	        $('#left-nav').show();
	    	
			//TOP NAV STYLING
	        $('#site-navigation').columns({
	            columnSelector: '.menu ul.root>li.static'
	        });

	        //need to delete any border on last li
	        $('#site-navigation .root>li').last().css("border-right-width", 0);

	        //whitehouse.gov navigation formatting, columns within navigation drop downs
	        //commented out to show the nav options to emm.  also added style float: none; to #site-navigation li
	        //$('#site-navigation').formatNav({});
            $('#site-navigation').formatNav({expandLeft: false});
			
			/* superfish formatting */
	        $('.menu-horizontal ul').superfish({
	            delay: 500,
	            speed: 200,
	            autoArrows: false,
	            dropShadows: false
	        });
			
	        /*handle shortcuts click*/
	        $('#shortcuts').click(function () {
	            fillDrawer();
	            $('#site-top-drawer').slideToggle('slow');
	            //need to handle piping issues
	            fixPipes();
	        });

            /* puts keywords into meta info on edit mode */
			if ($('.ms-formfieldvaluecontainer').length != 0) { //the above if statement verifies that the page is in edit mode
				var keyText = "Keywords applied to this page (may contain page keys that were saved last time this page was edited): <br/>";
				keyText += $('meta[name=Keywords]').attr("content");
				$('#meta-key-list').html(keyText);
			}


	        /* web part editor moved up the page */
	        if ($('table.ms-ToolPaneOuter').length != 0) {
	            $('table.ms-ToolPaneOuter').css('position', 'absolute');
	            $('table.ms-ToolPaneOuter').css('top', $('table.ms-WPSelected').position().top);
	            var webpartLeft = $('table.ms-WPSelected').position().left;
	            var webpartWidth = $('table.ms-WPSelected').outerWidth();
	            $('table.ms-ToolPaneOuter').css('left', webpartLeft + webpartWidth);
	        }
	        
			/*sticky alert stuff*/
			if($('#dpu-alert').length>0){
